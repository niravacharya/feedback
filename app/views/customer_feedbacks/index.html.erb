<h1>Listing customer_feedbacks</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Pincode</th>
    <th>Date Of Birth</th>
    <th>Mobile number</th>
    <th>Phone no</th>
    <th>Email</th>
    <th>Education</th>
    <th>Occupation</th>
    <th>Jobpost</th>
    <th>Name of company</th>
    <th>Experience</th>
    <th>Income</th>
    <th>Economical liability</th>
    <th>Number of child</th>
    <th>Years intend to work</th>
    <th>Short term goals</th>
    <th>Long term goals</th>
    <th>Age of retirement</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @customer_feedbacks.each do |customer_feedback| %>
  <tr>
    <td><%= customer_feedback.name %></td>
    <td><%= customer_feedback.address %></td>
    <td><%= customer_feedback.pincode %></td>
    <td><%= customer_feedback.dob %></td>
    <td><%= customer_feedback.mobile_number %></td>
    <td><%= customer_feedback.phone_no %></td>
    <td><%= customer_feedback.email %></td>
    <td><%= customer_feedback.education %></td>
    <td><%= customer_feedback.occupation %></td>
    <td><%= customer_feedback.jobpost %></td>
    <td><%= customer_feedback.Name_of_company %></td>
    <td><%= customer_feedback.experience %></td>
    <td><%= customer_feedback.income %></td>
    <td><%= customer_feedback.economical_liability %></td>
    <td><%= customer_feedback.number_of_child %></td>
    <td><%= customer_feedback.years_intend_to_work %></td>
    <td><%= customer_feedback.short_term_goals %></td>
    <td><%= customer_feedback.long_term_goals %></td>
    <td><%= customer_feedback.age_of_retirement %></td>
    <td><%= link_to 'Show', customer_feedback %></td>
    <td><%= link_to 'Edit', edit_customer_feedback_path(customer_feedback) %></td>
    <td><%= link_to 'Destroy', customer_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

